{
  "name": "@lipemat/postcss-boilerplate",
  "version": "3.7.0",
  "description": "Dependencies and scripts for a no config grunt postcss compiler.",
  "author": "Mat Lipe",
  "license": "MIT",
  "engines": {
    "node": ">=14.17.6"
  },
  "bugs": {
    "url": "https://github.com/lipemat/postcss-boilerplate/issues"
  },
  "homepage": "https://github.com/lipemat/postcss-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lipemat/postcss-boilerplate.git"
  },
  "keywords": [
    "postcss",
    "boilerplate",
    "no-config"
  ],
  "files": [
    "Gruntfile.js",
    "bin/",
    "config/",
    "lib/",
    "helpers/",
    "scripts/",
    "src/"
  ],
  "bin": {
    "lipemat-postcss-boilerplate": "./bin/lipemat-postcss-boilerplate.js"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "dist": "lipemat-postcss-boilerplate lib",
    "lint": "lipemat-postcss-boilerplate lint",
    "postprepare": "lipemat-js-boilerplate fix-pnp",
    "prepublishOnly": "rm -rf lib/* && yarn run build",
    "start": "lipemat-postcss-boilerplate start",
    "test": "lipemat-js-boilerplate test --silent"
  },
  "css_folder": "css/",
  "file_name": "front-end",
  "dependencies": {
    "@lipemat/css-mqpacker": "^8.0.1",
    "@lipemat/stylelint-config": "^1.1.2",
    "@wordpress/browserslist-config": "^4.1.0",
    "browserslist": "^4.17.0",
    "clean-css": "^5.2.1",
    "cross-spawn": "^6.0.5",
    "css-modules-loader-core": "^1.1.0",
    "fs-extra": "^7.0.1",
    "grunt": "^1.0.4",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-postcss": "^0.9.0",
    "grunt-stylelint": "^0.10.1",
    "lodash": "^4.17.20",
    "postcss": "^7.0.14",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-custom-media": "^6.0.0",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-scss": "^2.0.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@lipemat/js-boilerplate": "^8.5.0",
    "jest": "^26"
  },
  "packageManager": "yarn@3.2.0"
}
